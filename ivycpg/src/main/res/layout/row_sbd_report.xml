<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/list_view_height_volume_report"
    android:background="@drawable/list_selector"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/visitno"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/sno_width"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="center"
        android:singleLine="true"
        android:textSize="@dimen/font_small">
    </TextView>

    <TextView
        android:id="@+id/storename"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/product_name_twenty_five_character"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="start"
        android:textSize="@dimen/font_small" />

    <TextView
        android:id="@+id/target_gp"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/five_character_width"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="end"
        android:textSize="@dimen/font_small" />

    <TextView
        android:id="@+id/actual_gp"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/five_character_width"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="end"
        android:textSize="@dimen/font_small" />

    <TextView
        android:id="@+id/merch"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/five_character_width"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="end"
        android:textSize="@dimen/font_small" />

    <TextView
        android:id="@+id/gs"
        android:textColor="@android:color/black"
        android:layout_width="@dimen/five_character_width"
        android:layout_height="@dimen/list_view_height_volume_report"
        android:gravity="end"
        android:textSize="@dimen/font_small" />

</LinearLayout>