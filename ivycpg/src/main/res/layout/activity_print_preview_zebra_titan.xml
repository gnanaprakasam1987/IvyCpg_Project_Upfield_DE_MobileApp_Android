                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:padding="5dp"
                            android:text=""
                            android:textColor="@android:color/black"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
