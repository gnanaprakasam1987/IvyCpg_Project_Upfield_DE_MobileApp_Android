                            android:layout_height="fill_parent"
                            android:cacheColorHint="@null"
                            android:divider="@android:color/white"
                            android:dividerHeight="1dp"
                            android:fitsSystemWindows="true"
                            android:visibility="gone" />
                    </LinearLayout>
