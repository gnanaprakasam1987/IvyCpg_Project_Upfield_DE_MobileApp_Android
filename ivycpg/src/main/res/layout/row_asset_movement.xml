<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/asset_dailog_ListRow"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/list_selector"
    android:gravity="center_vertical"
    android:weightSum="9"
    android:orientation="horizontal"
    android:paddingLeft="@dimen/list_padding_left_n_right"
    android:paddingRight="@dimen/list_padding_left_n_right"
    android:paddingBottom="@dimen/list_item_padding_top_nd_bottom"
    android:paddingTop="@dimen/list_item_padding_top_nd_bottom"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?attr/customListItemPrimary"
        android:layout_weight="4"
        android:id="@+id/txt_move_assetName"
        android:padding="@dimen/dimen_4dp"
        android:text="@string/app_name"
        android:maxLines="1"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="?attr/customListItemPrimary"
        android:layout_weight="4"
        android:id="@+id/txt_move_serialNumber"
        android:padding="@dimen/dimen_4dp"
        android:text="@string/app_name"
        android:maxLines="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/iv_move_icon"
        android:padding="@dimen/dimen_4dp"
        android:src="@drawable/ic_compare_arrows_black_24dp"/>



</LinearLayout>