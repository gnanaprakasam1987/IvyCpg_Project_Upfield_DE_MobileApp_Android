            style="@style/TextViewListItemSecondaryHighlight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/pname"
            android:layout_gravity="center"
            android:padding="3dp"
            android:text="@string/prod_code" />

        <TextView
            android:id="@+id/batchnumber"
            style="@style/TextViewListItemSecondaryHighlight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/pname"
            android:layout_gravity="center"
            android:layout_toEndOf="@+id/product_code"
            android:layout_toRightOf="@+id/product_code"
            android:padding="3dp"
            android:text="@string/batch_no" />
    </RelativeLayout>


