            style="@style/TextViewListItemPrimary"
            android:layout_width="@dimen/price_product_name"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:ellipsize="marquee"
            android:maxLines="2"
            android:padding="3dp" />

