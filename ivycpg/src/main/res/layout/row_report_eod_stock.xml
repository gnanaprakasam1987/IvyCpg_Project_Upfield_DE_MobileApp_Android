<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/orderScreenListRow"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="40dp"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="@dimen/price_product_name"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <TextView
        android:id="@+id/pname"
        android:layout_width="@dimen/price_product_name"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:ellipsize="marquee"
        android:maxLines="2"
        android:padding="3dp"
        android:textSize="@dimen/font_small"
        android:textColor="@android:color/black"/>
        <TextView
            android:id="@+id/batchnumber"
            style="?textFieldStyle"
            android:layout_width="@dimen/price_product_name"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textStyle="normal"
            android:textSize="@dimen/font_nano_small"
            android:padding="3dp"
            android:text=""
            android:textColor="@android:color/black"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/loadstock_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/loadstock_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/loadstock"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_return_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/tv_return_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/tv_return"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/soldstock_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/soldstock_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>

            <TextView
                android:id="@+id/soldstock"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"/>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_freeissued"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/freeissued_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/freeissued_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/freeissued"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_replacement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/replacement_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/replacement_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
               />

            <TextView
                android:id="@+id/replacement"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/sih_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/sih_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/sih"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/empty_cs"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/empty_ou"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />

            <TextView
                android:id="@+id/empty"
                android:layout_width="@dimen/price_value"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:ellipsize="marquee"
                android:maxLines="2"
                android:padding="3dp"
                android:textSize="@dimen/font_small"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:text="0"
                />
        </LinearLayout>
    </RelativeLayout>


</LinearLayout>