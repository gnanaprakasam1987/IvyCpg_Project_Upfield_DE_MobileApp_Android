<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/retailer_card"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="70dp"
    android:layout_margin="5dp"
    card_view:cardCornerRadius="2dp"
    card_view:cardElevation="3dp">

    <TextView
        android:id="@+id/tv_retailername"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/profile_history_20dp"
        android:layout_gravity="center"
        android:layout_centerVertical="true"
        android:textSize="@dimen/font_small"
        android:textColor="@color/FullBlack"
        android:text="gdgvd fd dfg"
        android:layout_marginLeft="@dimen/profile_history_20dp" />
</android.support.v7.widget.CardView>
