                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/profile_margin_top_minus"
            android:orientation="horizontal"
            android:weightSum="4">

            <LinearLayout
                android:id="@+id/driver_name_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/driver_name_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="history_label_driver_name"
                        android:text="@string/driver_name" />

                    <TextView
                        android:id="@+id/driver_name_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/del_docno_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/del_docno_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="history_label_del_doc_no"
                        android:text="@string/delivery_doc_no" />

                    <TextView
                        android:id="@+id/del_docno_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/cust_pono_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/cust_pono_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="history_label_customer_po_no"
                        android:text="@string/customer_po_no" />

                    <TextView
                        android:id="@+id/cust_pono_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/delivery_date_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/delivery_date_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="history_label_del_date"
                        android:text="@string/delivery_date_label" />

                    <TextView
                        android:id="@+id/delivery_date_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="0.5"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/inv_view_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="@dimen/dimen_2dp"
            android:layout_gravity="center"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:layout_marginRight="@dimen/dimen_2dp">

            <TextView
                android:id="@+id/inv_view_btn"
                android:layout_width="@dimen/profile_history_25dp"
                android:layout_height="@dimen/profile_history_25dp"
