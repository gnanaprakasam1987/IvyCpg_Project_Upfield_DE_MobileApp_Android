                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:layout_marginLeft="@dimen/profile_history_10dp"
                    android:layout_marginStart="@dimen/profile_history_10dp"
                    android:layout_marginTop="@dimen/profile_margin_top_minus"
                    android:gravity="start"
                    android:text="Dummy Text"
                    android:textColor="?attr/primarycolor"
                    android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/date_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/date_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:gravity="start|top"
                        android:text="@string/date"
                        android:tag="history_label_start_date" />

                    <TextView
                        android:id="@+id/date_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/due_date_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/due_date_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:gravity="start|top"
