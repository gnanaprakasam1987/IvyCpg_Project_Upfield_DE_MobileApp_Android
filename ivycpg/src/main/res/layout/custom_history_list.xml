                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/del_status_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
