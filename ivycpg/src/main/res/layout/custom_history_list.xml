            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/profile_margin_top_minus"
            android:orientation="horizontal"
            android:weightSum="6">

            <LinearLayout
                android:id="@+id/del_date_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">


                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/del_date_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="RField1"
                        android:text="@string/delivery_date_label" />

                    <TextView
                        android:id="@+id/del_date_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/invoice_date_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/invoice_date_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="RField2"
                        android:text="@string/invoice_date" />

                    <TextView
                        android:id="@+id/invoice_date_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/invoice_qty_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/invoice_qty_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="RField3"
                        android:text="@string/invoice_qty" />

                    <TextView
                        android:id="@+id/invoice_qty_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/del_rep_code_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/del_rep_code_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:tag="RField4"
                        android:text="@string/delivery_rep_code" />

                    <TextView
                        android:id="@+id/del_rep_code_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/paid_amt_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/paid_amt_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="start|top"
                        android:text="@string/amount_paid" />

                    <TextView
                        android:id="@+id/paid_amt_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_gravity="start|center"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/bal_amt_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/bal_amt_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
