                    <TextView
                        android:id="@+id/due_date_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/total_lines_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/tot_lines_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:gravity="start|top"
                        android:text="@string/tot_line" />


                    <TextView
                        android:id="@+id/tota_lines_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=" "
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/tot_val_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/tot_val_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="start|top"
                        android:text="@string/total_value" />

                    <TextView
                        android:id="@+id/tot_val"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_margin_top_minus"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="bottom|start"
                        android:text=""
                        android:textColor="@color/FullBlack"
                        android:textSize="@dimen/text_size_primary" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/tot_vol_layout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:orientation="vertical">

                    <TextView
                        style="?attr/customListItemSecondary"
                        android:id="@+id/tot_vol_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center"
                        android:layout_marginBottom="@dimen/profile_history_10dp"
                        android:layout_marginStart="@dimen/profile_history_10dp"
                        android:layout_marginTop="@dimen/profile_history_10dp"
                        android:layout_marginLeft="@dimen/profile_history_10dp"
                        android:gravity="start|top"
                        android:text="@string/total_vol" />

                    <TextView
